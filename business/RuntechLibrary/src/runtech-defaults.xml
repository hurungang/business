<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="runtech.messageKey.success" value="runtech.messageKey.success"/>
	<package name="runtech-defaults" extends="struts-default">
		<interceptors>
			<!-- interceptor for authentication -->
			<interceptor-stack name="runtechBasicStack">
				<interceptor-ref name="store">
	     			<param name="operationMode">RETRIEVE</param>
	   			</interceptor-ref>
		    	<interceptor-ref name="staticParams"></interceptor-ref>
   	            <interceptor-ref name="params"/>
			    <interceptor-ref name="scopedModelDriven"></interceptor-ref>
			    <interceptor-ref name="modelDriven"></interceptor-ref>
	            <interceptor-ref name="params"/>
                <interceptor-ref name="servletConfig"/>
				<interceptor-ref name="store">
	     			<param name="operationMode">STORE</param>
	   			</interceptor-ref>
		    </interceptor-stack>
		</interceptors>
	</package>
</struts>
